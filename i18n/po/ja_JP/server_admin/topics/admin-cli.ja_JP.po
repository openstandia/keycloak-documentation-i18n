msgstr ""
"$ kcadm.sh create user-federation/instances -r demorealm -s "
"providerName=kerberos -s priority=0 -s config.debug=false -s "
"config.allowPasswordAuthentication=true -s 'config.editMode=\"UNSYNCED\"' -s"
