msgid "<Keys>\n"
"   <Key signing=\"true\">\n"
"      <PrivateKeyPem>\n"
"         2341251234AB31234==231BB998311222423522334\n"
"      </PrivateKeyPem>\n"
"      <CertificatePem>\n"
"         211111341251234AB31234==231BB998311222423522334\n"
"      </CertificatePem>\n"
"   </Key>\n"
"</Keys>\n"
msgstr ""

