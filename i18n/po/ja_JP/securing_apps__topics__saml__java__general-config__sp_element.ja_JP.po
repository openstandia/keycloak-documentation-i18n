msgid "entityID"
msgstr ""

#. type: Title =====
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:2
#, no-wrap
msgid "SP Element"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:5
#, no-wrap
msgid "Here is the explanation of the SP element attributes: \n"
msgstr ""

#. type: delimited block -
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:17
#, no-wrap
msgid "<SP entityID=\"sp\"\n"
"    sslPolicy=\"ssl\"\n"
"    nameIDPolicyFormat=\"format\"\n"
"    forceAuthentication=\"true\"\n"
"    isPassive=\"false\"\n"
"    autodetectBearerOnly=\"false\">\n"
"...\n"
"</SP>\n"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:21
#, no-wrap
msgid "This is the identifier for this client.\n"
"The IdP needs this value to determine who the client is that is communicating with it. This setting is _REQUIRED_.\n"
msgstr ""

#. type: Labeled list
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:22
#, no-wrap
msgid "sslPolicy"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:29
#, no-wrap
msgid "This is the SSL policy the adapter will enforce.\n"
"Valid values are: `ALL`, `EXTERNAL`, and `NONE`.\n"
"For `ALL`, all requests must come in via HTTPS.\n"
"For `EXTERNAL`, only non-private IP addresses must come over the wire via HTTPS.\n"
"For `NONE`, no requests are required to come over via HTTPS.\n"
"This setting is _OPTIONAL_. Default value is `EXTERNAL`.\n"
msgstr ""

#. type: Labeled list
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:30
#, no-wrap
msgid "nameIDPolicyFormat"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:36
#, no-wrap
msgid "SAML clients can request a specific NameID Subject format.\n"
"Fill in this value if you want a specific format.\n"
"It must be a standard SAML format identifier: `urn:oasis:names:tc:SAML:2.0:nameid-format:transient`.\n"
"This setting is _OPTIONAL_.\n"
"By default, no special format is requested. \n"
msgstr ""

#. type: Labeled list
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:37
#, no-wrap
msgid "forceAuthentication"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:41
#, no-wrap
msgid "SAML clients can request that a user is re-authenticated even if they are already logged in at the IdP.\n"
"Set this to `true` to enable. This setting is _OPTIONAL_.\n"
"Default value is `false`.\n"
msgstr ""

#. type: Labeled list
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:42
#, no-wrap
msgid "isPassive"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:47
#, no-wrap
msgid "SAML clients can request that a user is never asked to authenticate even if they are not logged in at the IdP.\n"
"Set this to `true` if you want this.\n"
"Do not use together with `forceAuthentication` as they are opposite. This setting is _OPTIONAL_.\n"
"Default value is `false`.\n"
msgstr ""

#. type: Labeled list
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:48
#, no-wrap
msgid "turnOffChangeSessionIdOnLogin"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:52
#, no-wrap
msgid "The session ID is changed by default on a successful login on some platforms to plug a security attack vector.\n"
"Change this to `true` to disable this.  It is recommended you do not turn it off.\n"
"Default value is `false`.\n"
msgstr ""

#. type: Labeled list
#: source/securing_apps/topics/saml/java/general-config/sp_element.adoc:53
#, no-wrap
msgid "autodetectBearerOnly"
msgstr ""

