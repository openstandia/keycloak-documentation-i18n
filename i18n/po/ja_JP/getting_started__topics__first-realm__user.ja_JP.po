msgid "Add User"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/first-realm/user.adoc:17
#: source/server_admin/topics/users/create-user.adoc:15
#, no-wrap
msgid "image:{project_images}/add-user.png[]\n"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/first-realm/user.adoc:19
#, no-wrap
msgid "The only required field is `Username`.  When you are finished, click *Save*.  The management page for your new user opens.\n"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/first-realm/user.adoc:21
#, no-wrap
msgid "The next step is to define a temporary password for your new user. Click the *Credentials* tab.\n"
msgstr ""

#. type: Block title
#: source/getting_started/topics/first-realm/user.adoc:22
#, no-wrap
msgid "Set Temporary Password"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/first-realm/user.adoc:24
#, no-wrap
msgid "image:{project_images}/credentials.png[]\n"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/first-realm/user.adoc:26
#, no-wrap
msgid "Type a new password and confirm it. A red *Reset Password* button is displayed.\n"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/first-realm/user.adoc:27
#, no-wrap
msgid "Click *Reset Password* to reset the user password to the new one you specified.  \n"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/first-realm/user.adoc:30
#, no-wrap
msgid "This password is temporary and the user will be required to change it after first login. You can make it permanent\n"
"by flipping the *Temporary* switch from *On* to *Off* before clicking *Reset Password*.\n"
msgstr ""

