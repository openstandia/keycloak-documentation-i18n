msgstr ""

#. type: Plain text
#: source/server_admin/topics/user-federation/sssd.adoc:142
msgid "To configure a federated SSSD store, complete the following steps:"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/user-federation/sssd.adoc:144
msgid "Navigate to the Administration Console."
msgstr ""

#. type: Plain text
#: source/server_admin/topics/user-federation/sssd.adoc:145
msgid "From the left menu, select *User Federation.*"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/user-federation/sssd.adoc:146
