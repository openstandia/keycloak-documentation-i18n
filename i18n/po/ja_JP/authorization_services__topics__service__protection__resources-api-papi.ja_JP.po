msgid "Managing Resources"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/service/protection/resources-api-papi.adoc:5
#, no-wrap
msgid "Resource servers can manage their resources remotely using a UMA-compliant endpoint.\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/service/protection/resources-api-papi.adoc:9
#, no-wrap
msgid "```bash\n"
"http://${host}:${port}/auth/realms/${realm_name}/authz/protection/resource_set\n"
"```\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/service/protection/resources-api-papi.adoc:11
#, no-wrap
msgid "This endpoint provides registration operations outlined as follows (entire path omitted for clarity):\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/service/protection/resources-api-papi.adoc:13
#, no-wrap
msgid "Create resource set description: POST /resource_set\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/service/protection/resources-api-papi.adoc:14
#, no-wrap
msgid "Read resource set description: GET /resource_set/{_id}\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/service/protection/resources-api-papi.adoc:15
#, no-wrap
msgid "Update resource set description: PUT /resource_set/{_id}\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/service/protection/resources-api-papi.adoc:16
#, no-wrap
msgid "Delete resource set description: DELETE /resource_set/{_id}\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/service/protection/resources-api-papi.adoc:17
#, no-wrap
msgid "List resource set descriptions: GET /resource_set\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/service/protection/resources-api-papi.adoc:18
#, no-wrap
msgid "List resource set descriptions using a filter: GET /resource_set?filter=${filter}\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/service/protection/resources-api-papi.adoc:19
#, no-wrap
msgid "For more information about the contract for each of these operations, see https://docs.kantarainitiative.org/uma/rec-oauth-resource-reg-v1_0_1.html[UMA Resource Set Registration].\n"
msgstr ""

