msgid ""
"$ cd $JETTY_HOME\n"
"$ unzip keycloak-saml-jetty92-adapter-dist.zip"
msgstr ""
"$ cd $JETTY_HOME\n"
"$ unzip keycloak-saml-jetty92-adapter-dist.zip"

msgid "Enable the keycloak module for your jetty.base."
