"        ]\n"
"      },\n"
"      {\n"
"        \"name\" : \"Some Resource\",\n"
"        \"path\" : \"/usingPattern/{id}\",\n"
"        \"methods\" : [\n"
"          {\n"
"            \"method\": \"DELETE\",\n"
"            \"scopes\" : [\"urn:app.com:scopes:delete\"]\n"
"          }\n"
"        ]\n"
"      },\n"
"      {\n"
"        \"path\" : \"/exactMatch\"\n"
"      },\n"
"      {\n"
"        \"name\" : \"Admin Resources\",\n"
"        \"path\" : \"/usingWildCards/*\"\n"
"      }\n"
"    ]\n"
"  }\n"
"}\n"
msgstr ""
"{\n"
"  \"policy-enforcer\": {\n"
"    \"user-managed-access\" : {},\n"
"    \"enforcement-mode\" : \"ENFORCING\"\n"
"    \"paths\": [\n"
"      {\n"
