#. type: Title ===
#: source/server_admin/topics/overview/features.adoc:2
#, no-wrap
msgid "Features"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:5
#, no-wrap
msgid "Single-Sign On and Single-Sign Out for browser applications.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:6
#, no-wrap
msgid "OpenID Connect support.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:7
#, no-wrap
msgid "OAuth 2.0 support.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:8
#, no-wrap
msgid "SAML support.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:9
#, no-wrap
msgid "Identity Brokering - Authenticate with external OpenID Connect or SAML Identity Providers.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:10
#, no-wrap
msgid "Social Login - Enable login with Google, GitHub, Facebook, Twitter, and other social networks.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:11
#, no-wrap
msgid "User Federation - Sync users from LDAP and Active Directory servers.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:12
#, no-wrap
msgid "Kerberos bridge - Automatically authenticate users that are logged-in to a Kerberos server.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:13
#, no-wrap
msgid "Admin Console for central management of users, roles, role mappings, clients and configuration.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:14
#, no-wrap
msgid "Account Management console that allows users to centrally manage their account.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:15
#, no-wrap
msgid "Theme support - Customize all user facing pages to integrate with your applications and branding.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:16
#, no-wrap
msgid "Two-factor Authentication - Support for TOTP/HOTP via Google Authenticator or FreeOTP.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:17
#, no-wrap
msgid "Login flows - optional user self-registration, recover password, verify email, require password update, etc.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:18
#, no-wrap
msgid "Session management - Admins and users themselves can view and manage user sessions.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:19
#, no-wrap
msgid "Token mappers - Map user attributes, roles, etc. how you want into tokens and statements.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:20
#, no-wrap
msgid "Not-before revocation policies per realm, application and user.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:22
#, no-wrap
msgid "CORS support - Client adapters have built-in support for CORS.\n"
"ifeval::[{project_community}==true]\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:24
#, no-wrap
msgid "Service Provider Interfaces (SPI) - A number of SPIs to enable customizing various aspects of the server. Authentication flows, user federation providers,\n"
"protocol mappers and many more.\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:27
#, no-wrap
msgid "Client adapters for JavaScript applications, WildFly, JBoss EAP, Fuse, Tomcat, Jetty, Spring, etc.\n"
"endif::[]\n"
"ifeval::[{project_product}==true]\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:29
#, no-wrap
msgid "Client adapters for JavaScript applications, JBoss EAP, Fuse, etc.\n"
"endif::[]\n"
msgstr ""

#. type: Plain text
#: source/server_admin/topics/overview/features.adoc:29
#, no-wrap
msgid "Supports any platform/language that has an OpenID Connect Resource Provider library or SAML 2.0 Service Provider library.\n"
msgstr ""

