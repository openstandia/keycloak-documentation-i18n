"    image: docker.elastic.co/kibana/kibana-oss:6.8.2\n"
"    ports:\n"
"      - \"5601:5601\"\n"
"    networks:\n"
"      - elk\n"
"    depends_on:\n"
"      - elasticsearch\n"
"\n"
"networks:\n"
