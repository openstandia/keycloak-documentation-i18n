msgstr ""

#. type: Plain text
#: source/authorization_services/topics/resource-server/create-client.adoc:19
#, no-wrap
msgid "Type the `Client ID` of the client. For example, _my-resource-server_.\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/resource-server/create-client.adoc:20
#, no-wrap
msgid "Type the `Root URL` for your application. For example:\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/resource-server/create-client.adoc:24
#, no-wrap
msgid "```bash\n"
"http://${host}:${port}/my-resource-server\n"
"```\n"
msgstr ""

