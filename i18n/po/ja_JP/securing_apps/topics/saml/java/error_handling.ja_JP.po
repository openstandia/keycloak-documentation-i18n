#. type: Title ====
#, no-wrap
msgid "Error Handling"
msgstr "エラー処理"

#. type: delimited block -
#, no-wrap
msgid "<error-page>\n"
"    <error-code>403</error-code>\n"
"    <location>/ErrorHandler</location>\n"
"</error-page>\n"
"----    \n"
msgstr "<error-page>\n"
"    <error-code>403</error-code>\n"
