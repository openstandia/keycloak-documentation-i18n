"kind: KeycloakRealm\n"
"metadata:\n"
"  name: external-realm\n"
"  labels:\n"
"    app: external-keycloak\n"
"spec:\n"
"  unmanaged: true\n"
"  realm:\n"
"    id: \"basic\"\n"
"    realm: \"basic\"\n"
"  instanceSelector:\n"
"    matchLabels:\n"
"      app: external-keycloak"

