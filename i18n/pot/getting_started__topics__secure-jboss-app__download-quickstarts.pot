#: source/getting_started/topics/secure-jboss-app/download-quickstarts.adoc:14
#: source/authorization_services/topics/getting-started/hello-world/deploy.adoc:16
msgid ""
"ifeval::[{project_community}==true] You can obtain the code by cloning the "
"repository at {quickstartRepo_link}. The quickstarts are designed to work "
"with the most recent Keycloak release.  endif::[]"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/secure-jboss-app/download-quickstarts.adoc:18
#: source/authorization_services/topics/getting-started/hello-world/deploy.adoc:20
msgid ""
"ifeval::[{project_product}==true] You can obtain the code by cloning the "
"repository at {quickstartRepo_link}. Use the branch matching the version of "
"{project_name} in use.  endif::[]"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/secure-jboss-app/download-quickstarts.adoc:20
msgid ""
"Follow these steps to download the code, build it, and deploy it. Make sure "
"your {appserver_name} application server is started before you run these "
"steps."
msgstr ""

#. type: Block title
#: source/getting_started/topics/secure-jboss-app/download-quickstarts.adoc:21
#: source/authorization_services/topics/getting-started/hello-world/deploy.adoc:23
#, no-wrap
msgid "Clone Project"
msgstr ""

#. type: delimited block -
#: source/getting_started/topics/secure-jboss-app/download-quickstarts.adoc:27
#, no-wrap
msgid ""
"$ git clone {quickstartRepo_link}\n"
"$ cd {quickstartRepo_dir}/app-profile-jee-vanilla\n"
"$ mvn clean wildfly:deploy\n"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/secure-jboss-app/download-quickstarts.adoc:30
msgid ""
"You should see some text scroll down in the application server console "
"window.  After the application is successfully deployed go to:"
msgstr ""

#. type: Plain text
#: source/getting_started/topics/secure-jboss-app/download-quickstarts.adoc:32
msgid "http://localhost:8080/vanilla"
msgstr ""

