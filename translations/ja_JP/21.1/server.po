"    ports:\n"
"      - \"5601:5601\"\n"
"    networks:\n"
"      - elk\n"
"    depends_on:\n"
"      - elasticsearch\n"
"\n"
"networks:\n"
"  elk:\n"
"    driver: bridge"
msgstr ""
"# Launch Elasticsearch\n"
"version: '3.8'\n"
"\n"
"services:\n"
"  elasticsearch:\n"
"    image: docker.elastic.co/elasticsearch/elasticsearch-oss:6.8.2\n"
"    ports:\n"
"      - \"9200:9200\"\n"
"      - \"9300:9300\"\n"
"    environment:\n"
"      ES_JAVA_OPTS: \"-Xms512m -Xmx512m\"\n"
"    networks:\n"
"      - elk\n"
"\n"
"  logstash:\n"
"    image: docker.elastic.co/logstash/logstash-oss:6.8.2\n"
