msgid "The first step to enable {project_name} Authorization Services is to create the client application that you want to turn into a resource server.\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/resource-server/create-client.adoc:7
#, no-wrap
msgid "To create a client application, complete the following steps:\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/resource-server/create-client.adoc:9
#, no-wrap
msgid "Click *Clients*.\n"
msgstr ""

#. type: Plain text
#: source/authorization_services/topics/resource-server/create-client.adoc:12
#, no-wrap
