#. type: Block title
#, no-wrap
msgid "Wildfly 11"
msgstr "Wildfly 11"

#. type: Block title
#, no-wrap
msgid "Any other server but Wildfly 11"
msgstr ""

#. type: Title =====
#, no-wrap
msgid "Adapter Installation"
msgstr ""

#. type: Plain text
msgid "Each adapter is a separate download on the {project_name} download site."
msgstr ""

#. type: Plain text
msgid "Install on Wildfly 9 or 10, 11 or JBoss EAP 7:"
msgstr ""

#. type: delimited block -
#, no-wrap
msgid "$ cd $WILDFLY_HOME\n"
"$ unzip keycloak-saml-wildfly-adapter-dist.zip\n"
msgstr "$ cd $WILDFLY_HOME\n"
"$ unzip keycloak-saml-wildfly-adapter-dist.zip\n"

#. type: Plain text
#| msgid "JBoss EAP 6.4"
msgid "Install on JBoss EAP 6.x:"
msgstr ""

#. type: delimited block -
#, no-wrap
msgid "$ cd $JBOSS_HOME\n"
"$ unzip keycloak-saml-eap6-adapter-dist.zip\n"
msgstr "$ cd $JBOSS_HOME\n"
"$ unzip keycloak-saml-eap6-adapter-dist.zip\n"

#. type: delimited block -
#, no-wrap
msgid "$ cd $JBOSS_HOME\n"
"$ unzip rh-sso-saml-eap6-adapter.zip\n"
msgstr "$ cd $JBOSS_HOME\n"
"$ unzip rh-sso-saml-eap6-adapter.zip\n"

#. type: Plain text
msgid "Install on JBoss EAP 7.x:"
msgstr ""

#. type: delimited block -
#, no-wrap
msgid "$ cd $JBOSS_HOME\n"
"$ unzip rh-sso-saml-eap7-adapter.zip\n"
msgstr "$ cd $JBOSS_HOME\n"
"$ unzip rh-sso-saml-eap7-adapter.zip\n"

#. type: Plain text
msgid "These zip files create new JBoss Modules specific to the Wildfly/JBoss EAP SAML Adapter within your Wildfly or JBoss EAP distro."
msgstr ""

#. type: Plain text
msgid "After adding the modules, you must then enable the {project_name} SAML Subsystem within your app server's server configuration: `domain.xml` or `standalone.xml`."
msgstr ""

#. type: Plain text
msgid "There is a CLI script that will help you modify your server configuration.  Start the server and run the script from the server's bin directory:"
msgstr ""

#. type: delimited block -
#, no-wrap
msgid "$ ./bin/jboss-cli.sh --file=adapter-elytron-install-saml.cli\n"
msgstr "$ cd bin\n"
"$ ./jboss-cli.sh --file=adapter-elytron-install-offline.cli\n"

#. type: delimited block -
#, no-wrap
msgid "$ cd $JBOSS_HOME/bin\n"
"$ jboss-cli.sh -c --file=adapter-install-saml.cli\n"
"----        \n"
"The script will add the extension, subsystem, and optional security-domain as described below. \n"
msgstr ""

#. type: delimited block -
#, no-wrap
msgid "[source,xml]\n"
msgstr "[source,xml]\n"

#. type: Plain text
msgid "<server xmlns=\"urn:jboss:domain:1.4\">"
msgstr "<server xmlns=\"urn:jboss:domain:1.4\">"

#. type: Plain text
#, no-wrap
msgid "    <extensions>\n"
"        <extension module=\"org.keycloak.keycloak-saml-adapter-subsystem\"/>\n"
"          ...\n"
"    </extensions>\n"
msgstr "    <extensions>\n"
"        <extension module=\"org.keycloak.keycloak-saml-adapter-subsystem\"/>\n"
"          ...\n"
"    </extensions>\n"

#. type: Plain text
#, no-wrap
msgid "    <profile>\n"
"        <subsystem xmlns=\"urn:jboss:domain:keycloak-saml:1.1\"/>\n"
"         ...\n"
"    </profile>\n"
"----    \n"
msgstr "    <profile>\n"
"        <subsystem xmlns=\"urn:jboss:domain:keycloak-saml:1.1\"/>\n"
"         ...\n"
"    </profile>\n"
"----    \n"

#. type: Plain text
msgid "The `keycloak` security domain should be used with EJBs and other components when you need the security context created in the secured web tier to be propagated to the EJBs (other EE component) you are invoking.  Otherwise this configuration is optional."
msgstr ""

#. type: delimited block -
#, no-wrap
msgid "<server xmlns=\"urn:jboss:domain:1.4\">\n"
" <subsystem xmlns=\"urn:jboss:domain:security:1.2\">\n"
"    <security-domains>\n"
"...\n"
"      <security-domain name=\"keycloak\">\n"
"         <authentication>\n"
"           <login-module code=\"org.keycloak.adapters.jboss.KeycloakLoginModule\"\n"
"                         flag=\"required\"/>\n"
"          </authentication>\n"
"      </security-domain>\n"
"    </security-domains>\n"
msgstr "<server xmlns=\"urn:jboss:domain:1.4\">\n"
" <subsystem xmlns=\"urn:jboss:domain:security:1.2\">\n"
"    <security-domains>\n"
"...\n"
"      <security-domain name=\"keycloak\">\n"
"         <authentication>\n"
"           <login-module code=\"org.keycloak.adapters.jboss.KeycloakLoginModule\"\n"
"                         flag=\"required\"/>\n"
"          </authentication>\n"
"      </security-domain>\n"
"    </security-domains>\n"

#. type: Plain text
msgid "For example, if you have a JAX-RS service that is an EJB within your WEB-INF/classes directory, you'll want to annotate it with the `@SecurityDomain` annotation as follows:"
msgstr ""

#. type: delimited block -
#, no-wrap
msgid "import org.jboss.ejb3.annotation.SecurityDomain;\n"
"import org.jboss.resteasy.annotations.cache.NoCache;\n"
msgstr "import org.jboss.ejb3.annotation.SecurityDomain;\n"
"import org.jboss.resteasy.annotations.cache.NoCache;\n"

#. type: delimited block -
#, no-wrap
msgid "import javax.annotation.security.RolesAllowed;\n"
"import javax.ejb.EJB;\n"
"import javax.ejb.Stateless;\n"
"import javax.ws.rs.GET;\n"
"import javax.ws.rs.Path;\n"
"import javax.ws.rs.Produces;\n"
"import java.util.ArrayList;\n"
"import java.util.List;\n"
msgstr "import javax.annotation.security.RolesAllowed;\n"
"import javax.ejb.EJB;\n"
"import javax.ejb.Stateless;\n"
"import javax.ws.rs.GET;\n"
"import javax.ws.rs.Path;\n"
"import javax.ws.rs.Produces;\n"
"import java.util.ArrayList;\n"
"import java.util.List;\n"

#. type: delimited block -
#, no-wrap
msgid "@Path(\"customers\")\n"
"@Stateless\n"
"@SecurityDomain(\"keycloak\")\n"
"public class CustomerService {\n"
msgstr "@Path(\"customers\")\n"
"@Stateless\n"
"@SecurityDomain(\"keycloak\")\n"
"public class CustomerService {\n"

#. type: delimited block -
#, no-wrap
msgid "    @EJB\n"
"    CustomerDB db;\n"
msgstr "    @EJB\n"
"    CustomerDB db;\n"

#. type: delimited block -
#, no-wrap
msgid "    @GET\n"
"    @Produces(\"application/json\")\n"
"    @NoCache\n"
"    @RolesAllowed(\"db_user\")\n"
"    public List<String> getCustomers() {\n"
"        return db.getCustomers();\n"
"    }\n"
"}\n"
msgstr "    @GET\n"
"    @Produces(\"application/json\")\n"
"    @NoCache\n"
"    @RolesAllowed(\"db_user\")\n"
"    public List<String> getCustomers() {\n"
"        return db.getCustomers();\n"
"    }\n"
"}\n"

#. type: Plain text
msgid "We hope to improve our integration in the future so that you don't have to specify the `@SecurityDomain` annotation when you want to propagate a keycloak security context to the EJB tier."
msgstr ""

