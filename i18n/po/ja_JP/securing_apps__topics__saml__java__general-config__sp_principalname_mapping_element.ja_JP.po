#. type: Title =====
#: source/securing_apps/topics/saml/java/general-config/sp_principalname_mapping_element.adoc:2
#, no-wrap
msgid "SP PrincipalNameMapping element"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_principalname_mapping_element.adoc:6
#, no-wrap
msgid "This element is optional.\n"
"When creating a Java Principal object that you obtain from methods such as `HttpServletRequest.getUserPrincipal()`, you can define what name is returned by the `Principal.getName()` method.\n"
msgstr ""

#. type: delimited block -
#: source/securing_apps/topics/saml/java/general-config/sp_principalname_mapping_element.adoc:12
#, no-wrap
msgid "<SP ...>\n"
"  <PrincipalNameMapping policy=\"FROM_NAME_ID\"/>\n"
"</SP>\n"
msgstr ""

#. type: delimited block -
#: source/securing_apps/topics/saml/java/general-config/sp_principalname_mapping_element.adoc:16
#, no-wrap
msgid "<SP ...>\n"
"  <PrincipalNameMapping policy=\"FROM_ATTRIBUTE\" attribute=\"email\" />\n"
"</SP>\n"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_principalname_mapping_element.adoc:22
#, no-wrap
msgid "The `policy` attribute defines the policy used to populate this value.\n"
"The possible values for this attribute are:\n"
msgstr ""

#. type: Labeled list
#: source/securing_apps/topics/saml/java/general-config/sp_principalname_mapping_element.adoc:23
#, no-wrap
msgid "FROM_NAME_ID"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_principalname_mapping_element.adoc:25
#, no-wrap
msgid "This policy just uses whatever the SAML subject value is.  This is the default setting\n"
msgstr ""

#. type: Labeled list
#: source/securing_apps/topics/saml/java/general-config/sp_principalname_mapping_element.adoc:25
#, no-wrap
msgid "FROM_ATTRIBUTE"
msgstr ""

#. type: Plain text
#: source/securing_apps/topics/saml/java/general-config/sp_principalname_mapping_element.adoc:27
#, no-wrap
msgid "This will pull the value from one of the attributes declared in the SAML assertion received from the server.\n"
"You'll need to specify the name of the SAML assertion attribute to use within the `attribute` XML attribute.\n"
msgstr ""

